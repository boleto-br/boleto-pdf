{
  "name": "boleto-pdf",
  "version": "0.2.3",
  "description": "boleto pdf generetor ",
  "main": "dist/index.js",
  "scripts": {
    "prettier": "prettier --single-quote --no-semi --no-bracket-spacing --trailing-comma none --write \"src/**/*.js\" --write \"test/**/*.js\" --write rollup.config.js",
    "pretest": "npm run prettier && xo --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rollup -c",
    "precommit": "npm test && npm run build",
    "coverage": "jest --coverage",
    "coveralls": "npm run coverage && cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boleto-br/boleto-pdf.git"
  },
  "keywords": [
    "boleto",
    "pdf",
    "js"
  ],
  "author": "Rafael Castro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boleto-br/boleto-pdf/issues"
  },
  "homepage": "https://github.com/boleto-br/boleto-pdf#readme",
  "dependencies": {
    "luxon": "^1.2.1",
    "pdfkit": "^0.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.4.1",
    "coveralls": "^3.0.1",
    "cp-file": "^6.0.0",
    "gzip-size": "^4.1.0",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "mockdate": "^2.0.2",
    "prettier": "^1.12.1",
    "pretty-bytes": "^5.0.0",
    "rollup": "^0.61.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-local-resolve": "^1.0.7",
    "xo": "^0.21.1"
  },
  "xo": {
    "semicolon": false,
    "space": true,
    "envs": [
      "node",
      "jest"
    ],
    "rules": {
      "no-mixed-operators": 0
    },
    "ignores": [
      "example/**"
    ]
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ],
      "stage-0"
    ]
  }
}
