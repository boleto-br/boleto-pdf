{
  "name": "boleto-pdf",
  "version": "0.2.1",
  "description": "boleto pdf generetor ",
  "main": "src/index.js",
  "scripts": {
    "prettier": "prettier --single-quote --no-semi --no-bracket-spacing --trailing-comma none --write \"src/**/*.js\" --write \"test/**/*.js\" --write rollup.config.js",
    "pretest": "npm run prettier && xo --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rollup -c",
    "precommit": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boleto-br/boleto-pdf.git"
  },
  "keywords": [
    "boleto",
    "pdf",
    "js"
  ],
  "author": "Rafael Castro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boleto-br/boleto-pdf/issues"
  },
  "homepage": "https://github.com/boleto-br/boleto-pdf#readme",
  "dependencies": {
    "moment": "^2.18.1",
    "pdfkit": "^0.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "mockdate": "^2.0.2",
    "prettier": "^1.6.1",
    "rollup": "^0.49.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-local-resolve": "^1.0.7",
    "xo": "^0.18.2"
  },
  "xo": {
    "semicolon": false,
    "space": true,
    "envs": [
      "node",
      "jest"
    ],
    "rules": {
      "no-mixed-operators": 0
    },
    "parser": "babel-eslint"
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ],
      "stage-0"
    ]
  }
}
